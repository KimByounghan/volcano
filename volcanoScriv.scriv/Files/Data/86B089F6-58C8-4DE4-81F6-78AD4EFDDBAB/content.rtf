\pard\plain \tx0\tx360\tx720\tx1080\tx1440\tx1800\tx2160\tx2880\tx3600\tx4320\fi360\ltrch\loch {\f0\fs24\b0\i0 ### 1. \loch\af0\hich\af0\dbch\af0\uc1\u45432\'3F\u54868\'3F \u51060\'3F\u54644\'3F\u50752\'3F \u44288\'3F\u47532\'3F: \u50864\'3F\u47532\'3F\u44032\'3F \u50724\'3F\u47000\'3F \u49332\'3F\u44592\'3F \u50948\'3F\u54620\'3F \u51204\'3F\u47029\'3F}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u54788\'3F\u45824\'3F \u49324\'3F\u54924\'3F\u50640\'3F\u49436\'3F 60\u45824\'3F\u51032\'3F \u47784\'3F\u49845\'3F\u51008\'3F \u44284\'3F\u44144\'3F\u50752\'3F \u53356\'3F\u44172\'3F \u45804\'3F\u46972\'3F\u51276\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u44060\'3F\u49440\'3F\u46108\'3F \u44277\'3F\u51473\'3F \u48372\'3F\u44148\'3F\u44284\'3F \u49324\'3F\u54924\'3F \u51088\'3F\u50896\'3F \u45909\'3F\u48516\'3F\u50640\'3F \u54217\'3F\u44512\'3F \u49688\'3F\u47749\'3F\u51060\'3F \u45720\'3F\u50612\'3F\u45208\'3F\u44256\'3F, 60\u45824\'3F\u51032\'3F \u44148\'3F\u44053\'3F \u49345\'3F\u53468\'3F\u50752\'3F \u44592\'3F\u45733\'3F\u51060\'3F \u51210\'3F\u51008\'3F \u49457\'3F\u51064\'3F\u44284\'3F \u44204\'3F\u51460\'3F\u47564\'3F\u53372\'3F \u51339\'3F\u50500\'3F\u51276\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u51060\'3F\u45716\'3F \u51064\'3F\u44036\'3F\u51032\'3F \u49373\'3F\u50528\'3F \u51452\'3F\u44592\'3F\u44032\'3F \u44600\'3F\u50612\'3F\u51276\'3F\u51020\'3F\u51012\'3F \u51032\'3F\u48120\'3F\u54616\'3F\u47728\'3F, \u51060\'3F\u50640\'3F \u46384\'3F\u46972\'3F \u45432\'3F\u45380\'3F\u51012\'3F \u49352\'3F\u47213\'3F\u44172\'3F \u51221\'3F\u51032\'3F\u54624\'3F \u54596\'3F\u50836\'3F\u49457\'3F\u51060\'3F \u49373\'3F\u44220\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u50696\'3F\u47484\'3F \u46308\'3F\u50612\'3F 1991\u45380\'3F\u50640\'3F\u45716\'3F 65\u49464\'3F\u51032\'3F \u44592\'3F\u45824\'3F \u50668\'3F\u47749\'3F\u51060\'3F 15\u45380\'3F\u50640\'3F \u48520\'3F\u44284\'3F\u54664\'3F\u51648\'3F\u47564\'3F, \u54788\'3F\u51116\'3F\u45716\'3F \u51060\'3F\u48372\'3F\u45796\'3F \u55016\'3F\u50476\'3F \u44600\'3F\u50612\'3F\u51256\'3F 70\u45824\'3F \u54980\'3F\u48152\'3F, \u49900\'3F\u51648\'3F\u50612\'3F 80\u45824\'3F\u44620\'3F\u51648\'3F \u44148\'3F\u44053\'3F\u54616\'3F\u44172\'3F \u49373\'3F\u54876\'3F\u54616\'3F\u45716\'3F \u49884\'3F\u45824\'3F\u44032\'3F \u46104\'3F\u50632\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u46384\'3F\u46972\'3F\u49436\'3F, \u50864\'3F\u47532\'3F\u45716\'3F \u50526\'3F\u51004\'3F\u47196\'3F 80\u49464\'3F\u44620\'3F\u51648\'3F\u45716\'3F \u54876\'3F\u48156\'3F\u54616\'3F\u44172\'3F \u44221\'3F\u51228\'3F \u54876\'3F\u46041\'3F\u51012\'3F \u54624\'3F \u49688\'3F \u51080\'3F\u51012\'3F \u51221\'3F\u46020\'3F\u47196\'3F \u44148\'3F\u44053\'3F\u51012\'3F \u50976\'3F\u51648\'3F\u54644\'3F\u50556\'3F \u54620\'3F\u45796\'3F\u45716\'3F \u51204\'3F\u51228\'3F \u54616\'3F\u50640\'3F \u49334\'3F\u51012\'3F \u44228\'3F\u54925\'3F\u54644\'3F\u50556\'3F \u54633\'3F\u45768\'3F\u45796\'3F.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u54616\'3F\u51648\'3F\u47564\'3F, \u45432\'3F\u54868\'3F\u50640\'3F \u45824\'3F\u54620\'3F \u51060\'3F\u54644\'3F\u45716\'3F \u50500\'3F\u51649\'3F\u46020\'3F \u47566\'3F\u51008\'3F \u49324\'3F\u46988\'3F\u46308\'3F\u50640\'3F\u44172\'3F \u54588\'3F\u49345\'3F\u51201\'3F\u51064\'3F \u49688\'3F\u51456\'3F\u50640\'3F \u47672\'3F\u47932\'3F\u47084\'3F \u51080\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u45432\'3F\u51064\'3F \u51032\'3F\u54617\'3F \u51204\'3F\u47928\'3F\u44032\'3F\u47196\'3F\u49436\'3F \u45432\'3F\u54868\'3F\u51032\'3F \u52769\'3F\u51221\'3F\u44284\'3F \u51060\'3F\u54644\'3F \u48169\'3F\u48277\'3F\u51012\'3F \u49444\'3F\u47749\'3F\u54644\'3F \u48372\'3F\u44192\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u50696\'3F\u47484\'3F \u46308\'3F\u50612\'3F, \u51473\'3F\u44256\'3F\u52264\'3F\u47484\'3F \u54217\'3F\u44032\'3F\u54624\'3F \u46412\'3F\u50752\'3F \u50976\'3F\u49324\'3F\u54616\'3F\u44172\'3F \u51064\'3F\u44036\'3F\u51032\'3F \u45432\'3F\u54868\'3F\u46020\'3F \u54217\'3F\u44032\'3F\u54624\'3F \u49688\'3F \u51080\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u51473\'3F\u44256\'3F\u52264\'3F\u51032\'3F \u49457\'3F\u45733\'3F \u44592\'3F\u47197\'3F\u48512\'3F\u47484\'3F \u53685\'3F\u54644\'3F \u52264\'3F\u51032\'3F \u49345\'3F\u53468\'3F\u47484\'3F \u52404\'3F\u53356\'3F\u54616\'3F\u45716\'3F \u44163\'3F\u52376\'3F\u47100\'3F, \u51032\'3F\u54617\'3F\u50640\'3F\u49436\'3F\u45716\'3F \u45432\'3F\u54868\'3F\u47484\'3F \u54217\'3F\u44032\'3F\u54616\'3F\u44592\'3F \u50948\'3F\u54644\'3F \u45796\'3F\u50577\'3F\u54620\'3F \u44148\'3F\u44053\'3F \u51648\'3F\u54364\'3F\u47484\'3F \u49324\'3F\u50857\'3F\u54633\'3F\u45768\'3F\u45796\'3F. \u51060\'3F \u51648\'3F\u54364\'3F\u46308\'3F\u51008\'3F \u44256\'3F\u54792\'3F\u50517\'3F \u50976\'3F\u47924\'3F, \u45817\'3F\u45544\'3F \u50976\'3F\u47924\'3F, \u48372\'3F\u54665\'3F \u49549\'3F\u46020\'3F, \u52404\'3F\u51473\'3F, \u44540\'3F\u50977\'3F\u47049\'3F, \u50501\'3F\u47141\'3F \u46321\'3F \u45796\'3F\u50577\'3F\u54620\'3F \u44592\'3F\u45733\'3F\u51201\'3F \u54028\'3F\u46972\'3F\u48120\'3F\u53552\'3F\u47484\'3F \u54252\'3F\u54632\'3F\u54633\'3F\u45768\'3F\u45796\'3F. \u51060\'3F\u47088\'3F \u45936\'3F\u51060\'3F\u53552\'3F\u46308\'3F\u51012\'3F \u44592\'3F\u48152\'3F\u51004\'3F\u47196\'3F \u44060\'3F\u51064\'3F\u51032\'3F \u44148\'3F\u44053\'3F \u49345\'3F\u53468\'3F\u47484\'3F \u51333\'3F\u54633\'3F\u51201\'3F\u51004\'3F\u47196\'3F \u54217\'3F\u44032\'3F\u54616\'3F\u47728\'3F, \u51060\'3F\u45716\'3F \u44263\'3F \u44536\'3F \u49324\'3F\u46988\'3F\u51032\'3F \u45432\'3F\u54868\'3F \u51221\'3F\u46020\'3F\u47484\'3F \u45208\'3F\u53440\'3F\u45253\'3F\u45768\'3F\u45796\'3F.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u45908\'3F \u45208\'3F\u50500\'3F\u44032\'3F, \u45432\'3F\u54868\'3F\u50752\'3F \u45432\'3F\u49632\'3F\u47484\'3F \u51060\'3F\u54644\'3F\u54616\'3F\u45716\'3F\u45936\'3F \u50837\'3F\u51312\'3F \u47784\'3F\u54805\'3F\u51012\'3F \u49324\'3F\u50857\'3F\u54616\'3F\u45716\'3F \u44163\'3F\u51012\'3F \u49440\'3F\u54840\'3F\u54633\'3F\u45768\'3F\u45796\'3F. \u51060\'3F \u47784\'3F\u54805\'3F\u50640\'3F\u49436\'3F \u49464\'3F \u44032\'3F\u51648\'3F \u51452\'3F\u50836\'3F \u49688\'3F\u46020\'3F\u44845\'3F\u51648\'3F\u44032\'3F \u51080\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u54616\'3F\u45208\'3F\u45716\'3F \u49884\'3F\u44036\'3F, \u46608\'3F \u45796\'3F\u47480\'3F \u54616\'3F\u45208\'3F\u45716\'3F \u50976\'3F\u51204\'3F\u51088\'3F, \u47560\'3F\u51648\'3F\u47561\'3F\u51008\'3F \u54980\'3F\u49373\'3F\u50976\'3F\u51204\'3F\u54617\'3F\u51201\'3F \u51648\'3F\u54805\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u54980\'3F\u49373\'3F\u50976\'3F\u51204\'3F\u54617\'3F\u51201\'3F \u51648\'3F\u54805\'3F\u51008\'3F \u44060\'3F\u51064\'3F\u51032\'3F \u49373\'3F\u54876\'3F \u49845\'3F\u44288\'3F\u44284\'3F \u54872\'3F\u44221\'3F\u51060\'3F \u50612\'3F\u46523\'3F\u44172\'3F \u45572\'3F\u51201\'3F\u46104\'3F\u50632\'3F\u45716\'3F\u51648\'3F\u47484\'3F \u45208\'3F\u53440\'3F\u45236\'3F\u47728\'3F, \u51060\'3F\u45716\'3F \u50864\'3F\u47532\'3F\u44032\'3F \u50612\'3F\u46523\'3F\u44172\'3F \u45721\'3F\u50612\'3F\u44032\'3F\u45716\'3F\u51648\'3F\u50640\'3F \u53360\'3F \u50689\'3F\u54693\'3F\u51012\'3F \u48120\'3F\u52841\'3F\u45768\'3F\u45796\'3F.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u44208\'3F\u47200\'3F\u51201\'3F\u51004\'3F\u47196\'3F, \u50864\'3F\u47532\'3F\u45716\'3F \u45432\'3F\u54868\'3F\u47484\'3F \u50612\'3F\u45712\'3F \u51221\'3F\u46020\'3F \u51228\'3F\u50612\'3F\u54624\'3F \u54596\'3F\u50836\'3F\u44032\'3F \u51080\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u51060\'3F\u45716\'3F \u44256\'3F\u51109\'3F \u45208\'3F\u51648\'3F \u50506\'3F\u44256\'3F \u50724\'3F\u47000\'3F \u49332\'3F\u44592\'3F \u50948\'3F\u54620\'3F \u51204\'3F\u51228\'3F \u51312\'3F\u44148\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u50864\'3F\u47532\'3F\u51032\'3F \u47800\'3F\u51060\'3F \u45432\'3F\u54868\'3F\u44032\'3F \u45916\'3F \u46108\'3F \u49345\'3F\u53468\'3F\u47196\'3F \u50976\'3F\u51648\'3F\u46104\'3F\u47732\'3F, \u45908\'3F \u44148\'3F\u44053\'3F\u54616\'3F\u44256\'3F \u54876\'3F\u46041\'3F\u51201\'3F\u51064\'3F \u49334\'3F\u51012\'3F \u50689\'3F\u50948\'3F\u54624\'3F \u49688\'3F \u51080\'3F\u51012\'3F \u44163\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u51060\'3F\u47084\'3F\u54620\'3F \u51060\'3F\u54644\'3F\u50752\'3F \u44288\'3F\u47532\'3F\u45716\'3F \u50864\'3F\u47532\'3F\u44032\'3F \u45908\'3F \u50724\'3F\u47000\'3F, \u45908\'3F \u44148\'3F\u44053\'3F\u54616\'3F\u44172\'3F \u49332\'3F \u49688\'3F \u51080\'3F\u45716\'3F \u48120\'3F\u47000\'3F\u47196\'3F \u45208\'3F\u50500\'3F\u44032\'3F\u45716\'3F \u45936\'3F \u44208\'3F\u51221\'3F\u51201\'3F\u51064\'3F \u50669\'3F\u54624\'3F\u51012\'3F \u54624\'3F \u44163\'3F\u51077\'3F\u45768\'3F\u45796\'3F.}}