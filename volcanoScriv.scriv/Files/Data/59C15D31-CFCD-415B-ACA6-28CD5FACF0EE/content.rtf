\pard\plain \tx0\tx360\tx720\tx1080\tx1440\tx1800\tx2160\tx2880\tx3600\tx4320\fi360\ltrch\loch {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u47113\'3F\u49828\'3F \u54644\'3F\u47532\'3F\u49832\'3F\u51008\'3F 1908\u45380\'3F 3\u50900\'3F 5\u51068\'3F\u50640\'3F \u50689\'3F\u44397\'3F \u47021\'3F\u52964\'3F\u49492\'3F\u50640\'3F\u49436\'3F \u53468\'3F\u50612\'3F\u45212\'3F \u50976\'3F\u47749\'3F\u54620\'3F \u48176\'3F\u50864\'3F\u47196\'3F, \u48376\'3F\u47749\'3F\u51008\'3F \u47112\'3F\u51648\'3F\u45328\'3F\u46300\'3F \u52880\'3F\u47532\'3F \u54644\'3F\u47532\'3F\u49832\'3F\u51077\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u45716\'3F \u51452\'3F\u47196\'3F \u47924\'3F\u45824\'3F\u50752\'3F \u49828\'3F\u53356\'3F\u47536\'3F\u50640\'3F\u49436\'3F \u54876\'3F\u46041\'3F\u54616\'3F\u47728\'3F \u53945\'3F\u50976\'3F\u51032\'3F \u50864\'3F\u50500\'3F\u54616\'3F\u44256\'3F \u51648\'3F\u51201\'3F\u51064\'3F \u47588\'3F\u47141\'3F\u51004\'3F\u47196\'3F \u51096\'3F \u50508\'3F\u47140\'3F\u51256\'3F \u51080\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u47113\'3F\u49828\'3F \u54644\'3F\u47532\'3F\u49832\'3F\u51008\'3F \u53945\'3F\u55176\'3F \u48036\'3F\u51648\'3F\u52972\'3F '\u47560\'3F\u51060\'3F \u54168\'3F\u50612\'3F \u47112\'3F\u51060\'3F\u46356\'3F'\u50640\'3F\u49436\'3F \u54760\'3F\u47532\'3F \u55176\'3F\u44596\'3F\u49828\'3F \u44368\'3F\u49688\'3F \u50669\'3F\u51012\'3F \u47585\'3F\u50500\'3F \u53360\'3F \u49324\'3F\u46993\'3F\u51012\'3F \u48155\'3F\u50520\'3F\u51004\'3F\u47728\'3F, \u51060\'3F \u50669\'3F\u54624\'3F\u47196\'3F 1964\u45380\'3F\u50640\'3F \u50500\'3F\u52852\'3F\u45936\'3F\u48120\'3F \u51452\'3F\u50672\'3F\u49345\'3F\u51012\'3F \u49688\'3F\u49345\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u44536\'3F\u51032\'3F \u50672\'3F\u44592\'3F \u49373\'3F\u54876\'3F\u51008\'3F 1930\u45380\'3F\u45824\'3F \u52488\'3F\u48152\'3F\u50640\'3F \u49884\'3F\u51089\'3F\u46104\'3F\u50632\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u52488\'3F\u44592\'3F\u50640\'3F\u45716\'3F \u50689\'3F\u44397\'3F\u50640\'3F\u49436\'3F \u45796\'3F\u49688\'3F\u51032\'3F \u50689\'3F\u54868\'3F\u50640\'3F \u52636\'3F\u50672\'3F\u54616\'3F\u47728\'3F \u44221\'3F\u47141\'3F\u51012\'3F \u49939\'3F\u50500\'3F\u44052\'3F\u44256\'3F, 1940\u45380\'3F\u45824\'3F\u50640\'3F \u46308\'3F\u50612\'3F\u49436\'3F\u45716\'3F \u54624\'3F\u47532\'3F\u50864\'3F\u46300\'3F\u47196\'3F \u47924\'3F\u45824\'3F\u47484\'3F \u50734\'3F\u44200\'3F \u45908\'3F \u53360\'3F \u49457\'3F\u44277\'3F\u51012\'3F \u44144\'3F\u46160\'3F\u50632\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u51032\'3F \u45824\'3F\u54364\'3F\u51089\'3F\u51004\'3F\u47196\'3F\u45716\'3F '\u48660\'3F\u47532\'3F\u49828\'3F \u49828\'3F\u54588\'3F\u47551\'3F' (1945), '\u45908\'3F \u44256\'3F\u49828\'3F\u53944\'3F \u50532\'3F \u48120\'3F\u49464\'3F\u49828\'3F \u48036\'3F\u50612\'3F' (1947), \u44536\'3F\u47532\'3F\u44256\'3F '\u53364\'3F\u47112\'3F\u50724\'3F\u54028\'3F\u53944\'3F\u46972\'3F' (1963) \u46321\'3F\u51060\'3F \u51080\'3F\u49845\'3F\u45768\'3F\u45796\'3F.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u47113\'3F\u49828\'3F \u54644\'3F\u47532\'3F\u49832\'3F\u51008\'3F \u46608\'3F\u54620\'3F \u48652\'3F\u47196\'3F\u46300\'3F\u50920\'3F\u51060\'3F \u47924\'3F\u45824\'3F\u50640\'3F\u49436\'3F\u46020\'3F \u54876\'3F\u48156\'3F\u55176\'3F \u54876\'3F\u46041\'3F\u54664\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u51032\'3F \u48652\'3F\u47196\'3F\u46300\'3F\u50920\'3F\u51060\'3F \u45936\'3F\u48596\'3F\u51089\'3F\u51008\'3F 1946\u45380\'3F\u51032\'3F '\u49828\'3F\u50948\'3F\u53944\'3F \u47672\'3F\u45908\'3F'\u50688\'3F\u51004\'3F\u47728\'3F, \u51060\'3F\u54980\'3F '\u47560\'3F\u51060\'3F \u54168\'3F\u50612\'3F \u47112\'3F\u51060\'3F\u46356\'3F' \u50808\'3F\u50640\'3F\u46020\'3F \u45796\'3F\u50577\'3F\u54620\'3F \u51089\'3F\u54408\'3F\u50640\'3F\u49436\'3F \u46832\'3F\u50612\'3F\u45212\'3F \u50672\'3F\u44592\'3F\u47141\'3F\u51012\'3F \u49440\'3F\u48372\'3F\u50688\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u53945\'3F\u55176\'3F \u44536\'3F\u45716\'3F \u47568\'3F\u45140\'3F\u54620\'3F \u50689\'3F\u44397\'3F \u49888\'3F\u49324\'3F\u51032\'3F \u51060\'3F\u48120\'3F\u51648\'3F\u47484\'3F \u53685\'3F\u54644\'3F \u51088\'3F\u49888\'3F\u47564\'3F\u51032\'3F \u46021\'3F\u53945\'3F\u54620\'3F \u52880\'3F\u47533\'3F\u53552\'3F\u47484\'3F \u44396\'3F\u52629\'3F\u54664\'3F\u51004\'3F\u47728\'3F, \u51060\'3F\u47084\'3F\u54620\'3F \u51060\'3F\u48120\'3F\u51648\'3F\u45716\'3F \u44536\'3F\u44032\'3F \u50672\'3F\u44592\'3F\u54620\'3F \u47566\'3F\u51008\'3F \u50669\'3F\u54624\'3F\u50640\'3F \u44618\'3F\u51060\'3F\u47484\'3F \u45908\'3F\u54644\'3F\u51452\'3F\u50632\'3F\u49845\'3F\u45768\'3F\u45796\'3F.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u47113\'3F\u49828\'3F \u54644\'3F\u47532\'3F\u49832\'3F\u51008\'3F \u46608\'3F\u54620\'3F \u44060\'3F\u51064\'3F \u49373\'3F\u54876\'3F\u50640\'3F\u49436\'3F \u50668\'3F\u47084\'3F \u52264\'3F\u47168\'3F \u44208\'3F\u54844\'3F\u44284\'3F \u51060\'3F\u54844\'3F\u51012\'3F \u44221\'3F\u54744\'3F\u54616\'3F\u47728\'3F \u54868\'3F\u51228\'3F\u51032\'3F \u51473\'3F\u49900\'3F\u50640\'3F \u49456\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u47084\'3F\u45208\'3F \u44536\'3F\u51032\'3F \u50672\'3F\u44592\'3F \u44221\'3F\u47141\'3F\u44284\'3F \u51204\'3F\u47928\'3F\u49457\'3F\u51008\'3F \u51060\'3F\u47084\'3F\u54620\'3F \u44060\'3F\u51064\'3F\u51201\'3F\u51064\'3F \u49334\'3F\u51032\'3F \u49548\'3F\u46976\'3F\u51012\'3F \u46832\'3F\u50612\'3F\u45336\'3F\u45716\'3F \u44163\'3F\u51004\'3F\u47196\'3F \u54217\'3F\u44032\'3F\u48155\'3F\u50520\'3F\u49845\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u45716\'3F 1990\u45380\'3F 6\u50900\'3F 2\u51068\'3F\u50640\'3F \u45684\'3F\u50837\'3F\u50640\'3F\u49436\'3F \u54224\'3F\u50516\'3F\u51004\'3F\u47196\'3F \u49324\'3F\u47581\'3F\u54664\'3F\u51004\'3F\u47728\'3F, \u44536\'3F\u51032\'3F \u51453\'3F\u51020\'3F\u51008\'3F \u50672\'3F\u44537\'3F\u44284\'3F \u50689\'3F\u54868\'3F\u44228\'3F\u50640\'3F \u53360\'3F \u49552\'3F\u49892\'3F\u47196\'3F \u50668\'3F\u44200\'3F\u51276\'3F\u49845\'3F\u45768\'3F\u45796\'3F.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u47113\'3F\u49828\'3F \u54644\'3F\u47532\'3F\u49832\'3F\u51032\'3F \u50696\'3F\u49696\'3F\u51201\'3F \u50976\'3F\u49328\'3F\u51008\'3F \u44536\'3F\u51032\'3F \u51089\'3F\u54408\'3F\u44284\'3F \u49688\'3F\u49345\'3F \u44221\'3F\u47141\'3F\u50640\'3F\u49436\'3F \u47749\'3F\u54869\'3F\u55176\'3F \u46300\'3F\u47084\'3F\u45225\'3F\u45768\'3F\u45796\'3F. \u44536\'3F\u45716\'3F \u51088\'3F\u49888\'3F\u51060\'3F \u47585\'3F\u51008\'3F \u45796\'3F\u50577\'3F\u54620\'3F \u50669\'3F\u54624\'3F\u51012\'3F \u53685\'3F\u54644\'3F \u49452\'3F\u49464\'3F\u54616\'3F\u44256\'3F \u51648\'3F\u51201\'3F\u51064\'3F \u50672\'3F\u44592\'3F\u47196\'3F \u44288\'3F\u44061\'3F\u50640\'3F\u44172\'3F \u44618\'3F\u51008\'3F \u51064\'3F\u49345\'3F\u51012\'3F \u45224\'3F\u44220\'3F\u51004\'3F\u47728\'3F, \u44536\'3F\u51032\'3F \u51060\'3F\u47492\'3F\u51008\'3F \u50724\'3F\u45720\'3F\u45216\'3F\u50640\'3F\u46020\'3F \u50672\'3F\u44537\'3F\u44284\'3F \u50689\'3F\u54868\'3F\u51032\'3F \u44256\'3F\u51204\'3F\u51201\'3F \u47749\'3F\u48176\'3F\u50864\'3F\u47196\'3F \u44592\'3F\u50613\'3F\u46121\'3F\u45768\'3F\u45796\'3F.}}